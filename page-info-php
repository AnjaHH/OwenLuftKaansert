<?php get_header () ?>


    <div class="container">
        <?php if(have_posts()): ?>
        <?php while(have_posts()): the_post() ?>

        <div class="info">
            <div id="info-name">

            </div>
            <div class="info-details">
                <div id="info-details">

                </div>
                <div id="info-image">

                </div>
            </div>
           
        </div>

     

<?php the_content() ?>
        <?php endwhile; ?>
        <?php else: ?>
            <p>Sorry, we did not find any content to share here.</p>
        <?php endif; ?>
    </div>



<?php get_footer () ?>